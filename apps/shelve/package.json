{
  "name": "@shelve/app",
  "description": "Shelve is a project management tool for developers teams",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare && bun run prisma:generate",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@iconify-json/lucide": "^1.2.12",
    "@iconify-json/simple-icons": "^1.2.11",
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui": "https://pkg.pr.new/@nuxt/ui@503f701",
    "@prisma/client": "^5.22.0",
    "@shelve/cli": "*",
    "@shelve/crypto": "*",
    "@shelve/types": "*",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^11.2.0",
    "nuxt": "^3.14.159",
    "nuxt-auth-utils": "^0.5.2",
    "nuxt-build-cache": "^0.1.1",
    "prisma": "^5.22.0",
    "resend": "^4.0.0",
    "typescript": "^5.6.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-sonner": "^1.2.5",
    "zod": "^3.23.8"
  }
