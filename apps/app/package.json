    "@nuxt/scripts": "^0.9.2",
    "@nuxt/ui": "^2.18.6",
    "@prisma/client": "^5.19.1",
    "@shelve/crypto": "workspace:*",
    "@shelve/types": "workspace:*",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/slim": "^3.5.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue-email/components": "^0.0.19",
    "@vue-email/render": "^0.0.9",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "eslint": "^9.11.0",
    "nuxt": "^3.13.2",
    "nuxt-auth-utils": "^0.3.9",
    "nuxt-build-cache": "^0.1.1",
    "prisma": "^5.19.1",
    "resend": "^4.0.0",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5",
    "vue-sonner": "^1.2.1"
  }
