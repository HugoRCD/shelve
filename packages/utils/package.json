{
  "name": "@shelve/utils",
  "version": "1.0.5",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:prepare": "unbuild",
    "build": "unbuild",
    "release": "bun run build && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=22.11.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "latest",
    "unbuild": "2.0.0"
  }
}
