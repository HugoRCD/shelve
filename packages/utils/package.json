{
  "name": "@shelve/utils",
  "version": "1.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:prepare": "unbuild",
    "build": "unbuild",
    "release": "bun run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "latest",
    "unbuild": "3.0.1"
  }
}
