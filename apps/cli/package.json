{
  "name": "@shelve/cli",
  "version": "0.1.4",
  "description": "The command-line interface for Shelve",
  "author": "HugoRCD",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/shelve.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/HugoRCD/shelve/issues"
  },
  "homepage": "https://github.com/HugoRCD/shelve#readme",
  "scripts": {
    "build": "automd && unbuild",
    "start": "node ./dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "release": "bun run build && npm publish",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@shelve/eslint-config": "*",
    "@shelve/types": "*",
    "@types/bun": "latest",
    "@types/semver": "^7.5.8",
    "automd": "^0.3.6",
    "eslint": "^8.57.0",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "typescript": "^5.4.3"
  },
  "bin": {
    "shelve": "dist/index.mjs",
    "shelve-cli": "dist/index.mjs"
  },
  "exports": {
    ".": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "citty": "^0.1.6",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "execa": "^8.0.1",
    "ofetch": "^1.3.4",
    "rc9": "^2.1.1",
    "semver": "^7.6.0",
    "unbuild": "^2.0.0"
  },
  "license": "Apache-2.0"
}
