{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".vercel/output/**",
        ".output/**",
        ".nuxt/**"
      ],
      "env": [
        "DATABASE_URL",
        "BETTER_AUTH_SECRET",
        "NUXT_BETTER_AUTH_SECRET",
        "REDIS_URL",
        "KV_URL",
        "KV_REST_API_TOKEN",
        "KV_REST_API_URL",
        "NUXT_SESSION_PASSWORD",
        "NUXT_PUBLIC_PAYLOAD_ID",
        "NUXT_PRIVATE_ENCRYPTION_KEY",
        "NUXT_OAUTH_GITHUB_CLIENT_ID",
        "NUXT_OAUTH_GITHUB_CLIENT_SECRET",
        "NUXT_OAUTH_GOOGLE_CLIENT_ID",
        "NUXT_OAUTH_GOOGLE_CLIENT_SECRET",
        "NUXT_PRIVATE_GITHUB_PRIVATE_KEY",
        "NUXT_PRIVATE_RESEND_API_KEY",
        "NUXT_PRIVATE_SENDER_EMAIL",
        "NUXT_PRIVATE_ADMIN_EMAILS",
        "NUXT_PRIVATE_ALLOWED_ORIGINS",
        "NUXT_PUBLIC_SITE_URL",
        "NUXT_PUBLIC_APP_URL",
        "NUXT_PUBLIC_API_URL",
        "NUXT_AUTH_EMAIL_ENABLED",
        "BOTID_BYPASS",
        "NUXT_DISABLE_BUILD_CACHE",
        "HUB_APPLY_MIGRATIONS_DURING_BUILD",
        "SKIP_ENV_VALIDATION"
      ]

    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": [
        "dist/**",
        ".output/**",
        ".nuxt/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:prepare": {
      "cache": false
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "lint": {
      "dependsOn": ["transit"]
    },
    "lint:fix": {},
    "test": {},
    "release": {},
    "typecheck": {
      "dependsOn": ["transit"]
    }
  }
}
