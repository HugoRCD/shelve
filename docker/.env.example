# Generated by Shelve CLI
NUXT_OAUTH_GITHUB_CLIENT_ID=your_value
NUXT_OAUTH_GITHUB_CLIENT_SECRET=your_value
NUXT_PRIVATE_ENCRYPTION_KEY=your_value
NUXT_SESSION_PASSWORD=your_value
NUXT_PRIVATE_ALLOWED_ORIGINS=your_value

# GitHub App
NUXT_PUBLIC_GITHUB_APP_NAME=your_value
# Must be converted from PKCS#1 to PKCS#8 (as WebCrypto API only supports PKCS#8), then base64 encoded
# See https://github.com/gr2m/universal-github-app-jwt#about-private-key-formats
# $ openssl pkcs8 -topk8 -inform PEM -outform PEM -nocrypt -in private-key.pem -out private-key-pkcs8.key
# $ cat private-key-pkcs8.key | base64
NUXT_PRIVATE_GITHUB_PRIVATE_KEY=your_value

# Auto generated if not set
DATABASE_URL=your_value
NUXT_PRIVATE_REDIS_URL=your_value

# Optional
NUXT_PRIVATE_RESEND_API_KEY=your_value
NUXT_PRIVATE_SENDER_EMAIL=your_value
NUXT_PRIVATE_ADMIN_EMAILS=your_value
