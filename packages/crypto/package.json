{
  "name": "@shelve/crypto",
  "version": "1.0.4",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:prepare": "unbuild",
    "build": "unbuild",
    "release": "bun run build && npm publish"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "typescript": "latest",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "uncrypto": "^0.1.3"
  }
}
