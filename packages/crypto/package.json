{
  "name": "@shelve/crypto",
  "version": "1.0.6",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:prepare": "unbuild",
    "build": "unbuild",
    "release": "bun run build && npm publish"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=22.11.0"
  },
  "devDependencies": {
    "typescript": "latest",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "uncrypto": "^0.1.3"
  }
}
