    "@nuxt/image": "1.8.1",
    "@nuxt/ui": "3.0.0-alpha.10",
    "@shelve/types": "*",
    "@shelve/utils": "*",
    "@vue-email/components": "0.0.21",
    "@vue-email/render": "0.0.9",
    "@vueuse/core": "12.2.0",
    "@vueuse/nuxt": "12.2.0",
    "drizzle-kit": "0.30.1",
    "drizzle-orm": "0.38.3",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.15",
    "nuxt": "3.15.0",
    "nuxt-auth-utils": "0.5.7",
    "nuxt-build-cache": "0.1.1",
    "pg": "8.13.1",
    "resend": "4.0.1",
