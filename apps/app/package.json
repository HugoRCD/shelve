{
  "name": "@shelve/app",
  "description": "Shelve is a project management tool for developers teams",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare && npm run prisma:generate --no-engine",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "vercel:env": "vercel env pull",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev"
  },
  "prisma": {
    "schema": "server/database/schema.prisma"
  },
  "devDependencies": {
    "@hrcd/eslint-config": "^2.0.0",
    "@iconify-json/lucide": "^1.1.194",
    "@nuxt/content": "^2.13.0",
    "@nuxt/fonts": "latest",
    "@nuxt/image": "latest",
    "@nuxt/ui": "^2.17.0",
    "@prisma/client": "^5.15.1",
    "@prisma/extension-accelerate": "^1.1.0",
    "@shelve/types": "*",
    "@shelve/utils": "*",
    "@tsparticles/engine": "^3.4.0",
    "@tsparticles/slim": "^3.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue-email/components": "^0.0.17",
    "@vue-email/render": "^0.0.9",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^9.5.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.12.2",
    "nuxt-build-cache": "latest",
    "prisma": "^5.15.1",
    "resend": "^3.3.0",
    "ua-parser-js": "^1.0.38",
    "vue": "latest",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "^4.3.3",
    "vue-sonner": "^1.1.3"
  }
}
