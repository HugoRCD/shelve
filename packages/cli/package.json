{
  "name": "@shelve/cli",
  "version": "2.5.2",
  "description": "The command-line interface for Shelve",
  "homepage": "https://shelve.cloud",
  "bugs": {
    "url": "https://github.com/HugoRCD/shelve/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/shelve.git",
    "directory": "packages/cli"
  },
  "funding": "https://github.com/sponsors/HugoRCD",
  "license": "Apache-2.0",
  "author": "HugoRCD",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "shelve": "dist/index.mjs",
    "shelve-cli": "dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "bun src/index.ts",
    "dev:prod": "NODE_ENV=production bun src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "bun run test && npm publish",
    "release:edge": "bun run test && npm publish --tag edge",
    "start": "node ./dist/index.mjs",
    "test": "bun run build && bun run typecheck && bun run lint && vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "c12": "^2.0.1",
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "npm-registry-fetch": "^18.0.2",
    "nypm": "^0.3.12",
    "ofetch": "^1.4.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@shelve/types": "*",
    "@types/bun": "^1.1.13",
    "@types/npm-registry-fetch": "^8.0.7",
    "@types/semver": "^7.5.8",
    "eslint": "^9.14.0",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.4"
  }
}
